{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error"
    }
}